{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/icons*.{png,jpg,gif}"]
  },
  "routes": [
    {
      "route": "index.html",
      "headers": {
        "cache-control": "no-cache, max-age=0, must-revalidate"
      }
    },
    {
      "route": "*.js",
      "headers": {
        "cache-control": "public, max-age=1209600"
      }
    }
  ]
}

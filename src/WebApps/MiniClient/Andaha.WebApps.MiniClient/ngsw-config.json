{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "backend",
      "urls": [
        "/shopping-api/bill",
        "/api/bill",
        "/shopping-api/billcategory",
        "/api/billcategory",
        "/shopping-api/expense",
        "/api/expense",
        "/shopping-api/expense/time-range",
        "/api/expense/time-range",
        "/collaboration-api/connection/established",
        "/api/connection/established",
        "/collaboration-api/connection/outgoing",
        "/api/connection/outgoing",
        "/collaboration-api/connection/incoming",
        "/api/connection/incoming"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "14d",
        "timeout": "2s"
      }
    }
  ]
}
